<h1>Editing seo_search_term</h1>

<% form_for(@seo_search_term) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :term %><br />
    <%= f.text_field :term %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @seo_search_term %> |
<%= link_to 'Back', seo_search_terms_path %>
