	</tr>
	<tr>
		<th class='term'><%=link_to 'Term', seo_search_terms_path(:sort_column=>'term', :sort_order=>@sort_order)%></th>
		<th class='domain_position' colspan='2'>
			<%=link_to "#{Seo::Config.my_site_name} Position", seo_search_terms_path(:sort_column=>'my_domain_position', :sort_order=>@sort_order)%><br/>
