		<th class='yahoo clicks'><%=link_to 'Clicks', seo_search_terms_path(:sort_column=>'yahoo_clicks', :sort_order=>@sort_order)%></th>
		<th class='yahoo max_cpc'><%=link_to 'Max CPC', seo_search_terms_path(:sort_column=>'yahoo_maxcpc', :sort_order=>@sort_order)%></th>
